material {
    name : bakedColor,
    parameters : [
        {
            type : sampler2d,
            name : texture
        },
        {
            type : float4,
            name : baseColor
        }
    ],
    requires : [
        uv0
    ],
    shadingModel : unlit,
    blending: transparent,
    culling : none
}

fragment {
    void material(inout MaterialInputs material) {
        prepareMaterial(material);
        vec4 color = texture(materialParams_texture, getUV0());

        material.baseColor = color;
    }
}
